using System;

class Program
{
    static void Main(string[] args)
    {
        int a = 10;
        int b = 38;
        int c = (31 - 5 * a) / b;
        Console.WriteLine(c);
    }
}
/*
 * Код не запускал, просто скопировал.
 * 5 * а = 5 * 10 = 50.
 * 31 - 50 = -19.
 * -19 / 38 = 0, так как это int, то есть берётся целая часть от всего числа.
 */
